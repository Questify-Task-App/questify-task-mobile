{
  "expo": {
    "name": "questify-task-mobile",
    "slug": "questify-task-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/quest.png",
    "scheme": "questifytaskmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/quest.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.caiqueoak.questifytaskmobile"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/quest.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/quest.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "41956b1e-6c50-4618-9a20-f89adb605a49"
      }
    }
  }
}